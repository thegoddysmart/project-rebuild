# Copy to .env and fill in values before deploying

# Database
DATABASE_URL=postgresql://user:password@host:5432/dbname?schema=public

# NextAuth
NEXTAUTH_URL=https://your-domain.com
NEXTAUTH_SECRET=replace-with-secure-random

# App secrets
WEBHOOK_SECRET_KEY=replace-with-webhook-secret

# Optional: analytics, third-party keys
# SENTRY_DSN=...
# NEXT_PUBLIC_SOME_KEY=...
